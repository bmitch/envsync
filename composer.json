{
    "name": "bmitch/envsync",
    "description": "Environment variable checker for Laravel.",
    "type": "package",
    "require": {
        "php": ">=5.6.4",
        "symfony/console": "^3.1"
    },
    "bin": [
        "bin/envsync"
    ],
    "require-dev": {
        "phpmd/phpmd" : "@stable",
        "squizlabs/php_codesniffer": "2.*",
        "phploc/phploc": "*",
        "sebastian/phpcpd": "*",
        "sensiolabs/security-checker": "^3.0",
        "phpunit/phpunit": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Bmitch\\Envsync\\": "src/"
        },
        "classmap": [
            "test/FileSystemTest.php"
        ]
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bill Mitchell",
            "email": "wkmitch@gmail.com"
        }
    ]
}
